{
	"name": "11ty3",
	"version": "1.0.0",
	"description": "Blog for Eric Carlisle : UI, UX Engineer",
	"engines": {
		"node": ">=14.0.0"
	},
	"main": "index.js",
	"scripts": {
		"prodbuild": "ELEVENTY_ENV=production npm-run-all -s clean webpack:build 11ty:build",
		"11ty:build": "npx @11ty/eleventy --config .eleventy.js",
		"11ty:serve": "npx @11ty/eleventy --config .eleventy.js --serve --incremental",
		"clean": "rm -rf _site/**/*",
		"debug:dev": "ELEVENTY_ENV=development DEBUG=Eleventy* npm-run-all -p webpack:watch 11ty:serve",
		"debug:prod": "ELEVENTY_ENV=production DEBUG=Eleventy* npm-run-all -p webpack:watch 11ty:serve",
		"serve:dev": "ELEVENTY_ENV=development npm-run-all -p webpack:watch 11ty:serve",
		"serve:prod": "ELEVENTY_ENV=production npm-run-all -p npm:clean webpack:build 11ty:serve",
		"webpack:build": "webpack --config webpack.config.js",
		"webpack:watch": "webpack --config webpack.config.js --watch"
	},
	"author": "Eric Carlisle",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0-beta-1",
		"@11ty/eleventy-img": "^2.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"@11ty/eleventy-plugin-webc": "^0.8.1",
		"@sherby/eleventy-plugin-files-minifier": "^1.1.1",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "^10.4.13",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"cssnano": "^5.1.14",
		"cssnano-preset-advanced": "^5.3.9",
		"eleventy-plugin-postcss": "^1.0.4",
		"esbuild-postcss": "^0.0.4",
		"esbuild-sass-plugin": "^2.4.5",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.11",
		"file-loader": "^6.2.0",
		"luxon": "^3.1.1",
		"mini-css-extract-plugin": "^2.7.2",
		"npm": "^9.3.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.21",
		"postcss-loader": "^7.0.2",
		"postcss-sort-media-queries": "^4.3.0",
		"prettier": "^2.8.1",
		"resolve-url-loader": "^5.0.0",
		"rollup": "^3.10.0",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.16.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"@11ty/is-land": "^3.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
